[flake8]
ignore = C101, A005
exclude =
    .git,
    __pycache__,
    venv
filename = *.py
accept-encodings = utf-8
inline-quotes = single
max-complexity = 12
max-linenumber = 500
max-line-length = 119
multiline-quotes = double
per-file-ignores =
    hygg/project/settings/templates/settings.dev.py:F821
    hygg/project/settings/docker.py:F821
    hygg/project/settings/envvars.py:F821
